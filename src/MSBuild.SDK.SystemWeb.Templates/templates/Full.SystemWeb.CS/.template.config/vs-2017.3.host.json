{
  "$schema": "http://json.schemastore.org/vs-2017.3.host",
  "name": {
    "text": "Web Application (Model-View-Controller)"
  },
  "description": {
    "text": "A project template for creating an ASP.NET 4.x application with example ASP.NET MVC5 Views and Controllers. This template can also be used for RESTful HTTP services."
  },
  "order": 1,
  "icon": "vs-2017.3/WebApplication.png",
  "learnMoreLink": "https://github.com/CZEMacLeod/MSBuild.SDK.SystemWeb",
  "isApi": false,
  "ports": [
    {
      "name": "HttpPort",
      "useHttps": false
    },
    {
      "name": "HttpsPort",
      "useHttps": true
    }
  ],
  "excludeLaunchSettings": false,
  "minFullFrameworkVersion": "4.6.1",
  "disableHttpsSymbol": "NoHttps"
}